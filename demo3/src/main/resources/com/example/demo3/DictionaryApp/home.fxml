<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.geometry.Insets?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.demo3.DictionaryApp.homeController"
            prefHeight="400.0" prefWidth="720.0">
    <children>
        <AnchorPane layoutY="-1.0"
                    prefHeight="48.0"
                    prefWidth="607.0"
                    style="-fx-background-color: #0866a9;">
            <children>
                <Text fill="#fffffff7"
                      layoutY="30.0"
                      strokeType="OUTSIDE"
                      strokeWidth="0.0"
                      text="Dictionary Application"
                      textAlignment="CENTER"
                      wrappingWidth="720.0">
                    <font>
                        <Font name="System Bold"
                              size="18.0" />
                    </font>
                </Text>
                <ImageView fitHeight="39.0"
                           fitWidth="30.0"
                           layoutX="484.0"
                           layoutY="9.0"
                           pickOnBounds="true"
                           preserveRatio="true">
                    <image>
                        <Image url="@../image/dictionary.png"/>
                    </image>
                </ImageView>
            </children>
        </AnchorPane>
        <AnchorPane layoutY="47.0"
                    prefHeight="430.0"
                    prefWidth="204.0">
            <children>
                <TextField fx:id="text"
                           layoutX="12.0"
                           layoutY="10.0"
                           prefHeight="44.0"
                           prefWidth="186.0">
                    <font>
                        <Font size="19.0" />
                    </font>
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <padding>
                        <Insets left="5.0"
                                right="30.0" />
                    </padding>
                </TextField>
                <ListView fx:id="listView"
                          layoutX="12.0"
                          layoutY="61.0"
                          prefHeight="357.0"
                          prefWidth="186.0"
                          stylesheets="@style.css"/>
                <ImageView fitHeight="30.0"
                           fitWidth="30.0"
                           layoutX="165.0"
                           layoutY="17.0"
                           pickOnBounds="true"
                           preserveRatio="true">
                    <image>
                        <Image url="@../image/lookup.png" />
                    </image>
                </ImageView>
            </children>
        </AnchorPane>
        <AnchorPane layoutX="200.0"
                    layoutY="47.0"
                    prefHeight="430.0"
                    prefWidth="520.0"
                    style="-fx-background-color: #FFFFFF;">
            <children>
                <Button layoutX="390.0"
                        layoutY="10.0"
                        mnemonicParsing="false"
                        onAction="#gototoAddWord"
                        prefHeight="39.0"
                        prefWidth="109.0"
                        stylesheets="@style.css"
                        text="Thêm từ"
                        textFill="WHITE">
                    <graphic>
                        <ImageView fitHeight="23.0"
                                   fitWidth="28.0"
                                   pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@../image/plus.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="changeButton"
                        layoutX="132.0"
                        layoutY="383.0"
                        mnemonicParsing="false"
                        onAction="#changeExplain"
                        prefHeight="39.0"
                        prefWidth="103.0"
                        stylesheets="@style.css"
                        text="Sửa từ"
                        textAlignment="CENTER"
                        textFill="WHITE">
                    <graphic>
                        <ImageView fitHeight="26.0"
                                   fitWidth="25.0"
                                   pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url ="@../image/tools.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="changeButton2"
                        graphicTextGap="5.0"
                        layoutX="245.0"
                        layoutY="383.0"
                        mnemonicParsing="false"
                        onAction="#deleteWord"
                        prefHeight="39.0"
                        prefWidth="103.0"
                        stylesheets="@style.css"
                        text="Xóa từ"
                        textFill="WHITE">
                    <graphic>
                        <ImageView fitHeight="22.0"
                                   fitWidth="29.0"
                                   pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@../image/delete.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Text fx:id="engLabel"
                      fill="#0000ff"
                      layoutX="12.0"
                      layoutY="36.0"
                      strokeType="OUTSIDE"
                      strokeWidth="0.0"
                      wrappingWidth="306.0">
                    <font>
                        <Font size="20.0"/>
                    </font>
                </Text>
                <Button fx:id="changeButton3"
                        alignment="CENTER"
                        contentDisplay="CENTER"
                        graphicTextGap="6.0"
                        layoutX="325.0"
                        layoutY="8.0"
                        mnemonicParsing="false"
                        onAction="#speak"
                        prefHeight="44.0"
                        prefWidth="53.0"
                        stylesheets="@style.css"
                        textAlignment="RIGHT"
                        textFill="#111010">
                    <cursor>
                        <Cursor fx:constant="HAND"/>
                    </cursor>
                    <graphic>
                        <ImageView fitHeight="27.0"
                                   fitWidth="71.0"
                                   pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@../image/megaphone.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <ScrollPane layoutX="12.0"
                            layoutY="62.0"
                            prefHeight="309.0"
                            prefWidth="490.0"
                            style="-fx-padding: 16;">
                    <content>
                        <Text fx:id="vietLabel"
                              strokeType="OUTSIDE"
                              strokeWidth="0.0"
                              wrappingWidth="372.0">
                            <font>
                                <Font size="18.0"/>
                            </font>
                        </Text>
                    </content>
                </ScrollPane>
                <Button layoutX="359.0"
                        layoutY="383.0"
                        mnemonicParsing="false"
                        onAction="#gotoTextTranslation"
                        prefHeight="39.0"
                        prefWidth="147.0"
                        stylesheets="@style.css"
                        text="Dịch văn bản">
                    <graphic>
                        <ImageView fitHeight="23.0"
                                   fitWidth="60.0"
                                   pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@../image/translation.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button layoutX="209.0"
                        layoutY="383.0"
                        mnemonicParsing="false"
                        onAction="#gotoGame"
                        prefHeight="39.0"
                        prefWidth="147.0"
                        stylesheets="@style.css"
                        text="Game">
                </Button>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
